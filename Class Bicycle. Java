public class Class Bicycle  {
	// Data members
	
	private String id;
	
	private String ownerName;
	
	// Constructors
	public Bicycle () {
		
		id = "xxxx-xxxx";
		ownerName = "Unassigned" 
	}
	public Bicycle (String tagNo,String name) {
		id = tagNo;
		ownerName = name;
	}
	// the rest of the class
}
